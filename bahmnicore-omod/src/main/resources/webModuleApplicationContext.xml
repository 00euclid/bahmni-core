<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
  		    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
  		    http://www.springframework.org/schema/context
  		    http://www.springframework.org/schema/context/spring-context-3.0.xsd">
    <context:component-scan base-package="org.bahmni.module.bahmnicore.web"/>

    <bean id="bahmniCoreProperties" class="org.bahmni.module.bahmnicore.web.properties.PropertiesReaderImpl" factory-method="load" scope="singleton"/>

    <bean id="bahmniEncounterSessionmatcher" class="org.bahmni.module.bahmnicore.matcher.EncounterSessionMatcher">
        <property name="administrationService" ref="adminService"/>
    </bean>

</beans>
